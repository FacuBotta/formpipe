# -----------------------------------------
# Protect php content folder
# -----------------------------------------

<FilesMatch ".*">
    Require all denied
</FilesMatch>

<Files "contact-form.php">
    Require all granted
</Files>

# -----------------------------------------
# Environment variables
# -----------------------------------------

SetEnv FORMPIPE_SMTP_HOST "smtp.gmail.com"

SetEnv FORMPIPE_SMTP_PORT "587"

SetEnv FORMPIPE_SMTP_USER ""

SetEnv FORMPIPE_SMTP_PASS ""

SetEnv FORMPIPE_FROM ""

SetEnv FORMPIPE_TO ""
